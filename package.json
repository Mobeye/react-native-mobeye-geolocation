{
  "name": "@mobeye/react-native-geolocation",
  "title": "React Native Mobeye Geolocation",
  "version": "1.6.0",
  "description": "Geolocation that used Fused Location Provider API for Android and Significant-Change service for iOS",
  "main": "lib/commonjs/index.js",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "README.md",
    "CHANGELOG.md",
    "android",
    "index.js",
    "ios",
    "react-native-mobeye-geolocation.podspec"
  ],
  "scripts": {
    "start": "react-native start",
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobeye/react-native-mobeye-geolocation.git",
    "baseUrl": "https://github.com/Mobeye/react-native-mobeye-geolocation"
  },
  "keywords": [
    "react-native",
    "geolocation"
  ],
  "author": {
    "name": "Mobeye",
    "company": "Mobeye"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "react-native-builder-bob": "^0.18.3",
    "@types/react-native": "^0.69.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "metro-react-native-babel-preset": "^0.70.3",
    "moment": "^2.29.4",
    "prettier": "^2.7.1",
    "react": "18.0.0",
    "react-native": "0.69.6",
    "typescript": "^4.8.4"
  },
  "react-native": "src/index.ts",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  }
